			</dir>
			<dir name="modules">
				<dir name="sfJqueryFormVal">
					<file role="data" name="actions/actions.class.php" />
					<file role="data" name="config/view.yml" />
					<file role="data" name="config/security.yml" />
					<file role="data" name="templates/indexSuccess.js.php" />
				</dir>
			</dir>
		</dir>
	</contents>
